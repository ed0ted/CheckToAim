@model List<CheckToAim.Models.PersonalAimList>

@{
    ViewBag.Title = "Complete CheckToAim";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Complete CheckToAim</h2>


<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.Id_ParentCheckList)*@
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Name)*@
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.Partial("ShowAims", item)
        </td>
        <td>
            
            @Html.ActionLink("Details", "CheckList", new { id=item.Id_ParentCheckList }) |
            @Html.ActionLink("Remove", "Delete_Completing", new { id=item.Id })
        </td>
    </tr>
}

</table>
